<!doctype html>
<!-- Website template by freewebsitetemplates.com -->
<html>
	{% include 'head.twig' %}
	<body>
		<div id="page">
			{% set selected = "planets" %}
			{% include 'navigation.twig' with selected %}
			<div id="body">
				<div class="header">
					<div>
						{% if objects %}
						    <h1>{{category}}</h1>
							<ul>
								{% for item in objects %}
								<li>
								    <a href="/moons/{{item.title}}"><img class="spaceimg" src="/uploades/{{item.image}}" alt="{{item.title}}"></a>
									<div>
										<h1>{{item.title}}</h1>
										<p>Description: {{item.description}}</p>
										{% if category == "planets" %}
										<a href="/planets/{{item.title}}" class="more">Read More</a>
										{% else %}
										<a href="/moons/{{item.title}}" class="more">Read More</a>
										{% endif %}
									</div>
								</li>
								{% endfor %}
							</ul>
						{% endif %}
					</div>
				</div>
			</div>
			{% include 'footer.twig' %}
		</div>
	</body>
</html>
